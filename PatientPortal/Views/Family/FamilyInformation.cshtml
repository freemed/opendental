@using System.Data;
@using OpenDentBusiness;
@using OpenDentBusiness.Mobile;
@model PatientPortalMVC.Models.FamilyModel
@{
	ViewBag.Title = "Family Information";
	Layout = "~/Views/Shared/_Layout.cshtml";
}


@if(Model.patList.Count()!=0) {
	
	<p><strong>Family Members:</strong></p>
	<div style="margin-left:3em">
	<table>
		<tr>
			<td><strong>Name</strong></td>
			<td><strong>Position</strong></td>
			<td><strong>Gender</strong></td>
			<td><strong>Status</strong></td>
			<td><strong>Age </strong></td>
		</tr>
	@foreach(var item in Model.patList) {  
		<tr>
			<td>@Html.Encode(item.LName),@Html.Encode(item.FName)</td>
			<td>@Html.Encode(item.Position)</td>
			<td>@Html.Encode(item.Gender)</td>
			<td>@Html.Encode(item.PatStatus)</td>
			<td>@Html.Encode(item.Age)</td>
		</tr>
 }
	</table>
	</div>
}



	<div style="padding-top:4em;">
	<div class="divtable">
	
		<div class="divtablerow">
			<div class="divtablecell1"><strong>Last Name:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.LName)
			</div>
		</div>
		<div class="divtablerow">
			<div class="divtablecell1"><strong>First Name:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.FName)
			</div>
		</div>
		<div class="divtablerow">
			<div class="divtablecell1"><strong>Middle Name:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.MiddleI)
			</div>
		</div>
		<div class="divtablerow">
			<div class="divtablecell1"><strong>Nick Name:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.Preferred)
			</div>
		</div>
		<div class="divtablerow">
			<div class="divtablecell1"><strong>Status:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.PatStatus)
			</div>
		</div>
		<div class="divtablerow">
			<div class="divtablecell1"><strong>Gender:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.Gender)
			</div>
		</div>
		<div class="divtablerow">
			<div class="divtablecell1"><strong>Birthdate:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.Birthdate.ToShortDateString())
			</div>
		</div>
		<div class="divtablerow">
			<div class="divtablecell1"><strong>Address:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.Address)
			</div>
		</div>			
		<div class="divtablerow">
			<div class="divtablecell1"><strong>Address2:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.Address2)
			</div>
		</div>
		<div class="divtablerow">
			<div class="divtablecell1"><strong>City:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.City)
			</div>
		</div>
		<div class="divtablerow">
			<div class="divtablecell1"><strong>State:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.State)
			</div>
		</div>
		<div class="divtablerow">
			<div class="divtablecell1"><strong>Zip:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.Zip)
			</div>
		</div>	
		<div class="divtablerow">
			<div class="divtablecell1"><strong>Home Phone:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.HmPhone)
			</div>
		</div>
		<div class="divtablerow">
			<div class="divtablecell1"><strong>Work Phone:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.WkPhone)
			</div>
		</div>
		<div class="divtablerow">
			<div class="divtablecell1"><strong>Cell Phone:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.WirelessPhone)
			</div> 
		</div>
		<div class="divtablerow">
			<div class="divtablecell1"><strong>Home Phone:</strong></div><div class="divtablecell2">@Html.Encode(Model.patm.Email)
			</div>
		</div>		
	</div>


	</div>
 
	
	
